<Application>
	<Head>
		<meta name="id" value="{{id}}"/>
		<meta name="title" value="{{name}}" version="1.0"/>
		<meta name="author" value="{{author}}" namespace="{{username}}"/>
		<meta name="license" value="AGPL-3.0"/>
		<meta name="repository" value="https://github.com/Defiant-System/{{id}}"/>
		<meta name="width" value="600"/>
		<meta name="height" value="400"/>
		<meta name="top" value="120"/>
		<meta name="left" value="120"/>
		<meta name="minimize"/>
		<meta name="maximize"/>
		<meta name="resize"/>
		<meta name="script" value="src/js/main.js"/>
		<meta name="style" value="src/css/main.less"/>
		<meta name="icons" value="src/svg/icons.svg"/>
		<meta name="content" value="src/partials/content.htm"/>

		@import "src/partials/menubar.xml"
	</Head>
</Application>