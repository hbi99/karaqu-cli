<MenuBar>
	<Menu name="{{name}}">
		<Menu name="About {{name}}"/>
		<Menu type="divider"/>
		<Menu name="New Game" click="restart"/>
		<Menu type="divider"/>
		<Menu name="Hide {{name}}"/>
		<Menu name="Hide Others"/>
		<Menu name="Show All" disabled="1"/>
		<Menu type="divider"/>
		<Menu name="Quit {{name}}" shell="win -c"/>
	</Menu>
	<Menu name="Help">
		<Menu name="Help" click="open-help"/>
		<Menu type="divider"/>
		<Menu name="Source Code"/>
	</Menu>
</MenuBar>